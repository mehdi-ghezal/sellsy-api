{
    "name": "mehdi-ghezal/sellsy-api",
    "description": "PHP wrapper for Sellsy API",
    "keywords": ["sellsy", "api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehdi Ghezal",
            "email": "mehdi.ghezal@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "nategood/httpful": "*",
        "guzzlehttp/guzzle": "~6.1",
        "symfony/expression-language": "~3.0",
        "doctrine/instantiator": "~1.0",
        "symfony/yaml": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Sellsy\\": "src/Sellsy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sellsy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
