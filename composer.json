{
    "name": "mehdi-ghezal/sellsy-api",
    "description": "PHP wrapper for Sellsy API",
    "keywords": ["sellsy", "api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehdi Ghezal",
            "email": "mehdi.ghezal@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "minime/annotations": "~2.0",
        "nategood/httpful": "*",
        "guzzlehttp/guzzle": "~6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "monolog/monolog": "~1.17",
        "rtheunissen/guzzle-log-middleware": "0.4.*"
    },
    "autoload": {
        "psr-4": {
            "Sellsy\\": "src/Sellsy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sellsy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
